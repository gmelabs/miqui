#!/usr/bin/env bash

export COMMAND='yum update -y'

eval $COMMAND 2>&1 | sed "s/^/$HOSTNAME: /"

for host in `cat <<EOF
master01.bigdata
worker01.bigdata
worker02.bigdata
lab01.bigdata
lab02.bigdata
EOF`
do
